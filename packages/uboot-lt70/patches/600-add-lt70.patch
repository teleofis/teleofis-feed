diff --git a/arch/mips/dts/teleofis_lt70.dts b/arch/mips/dts/teleofis_lt70.dts
new file mode 100644
index 0000000..7254b80
--- /dev/null
+++ b/arch/mips/dts/teleofis_lt70.dts
@@ -0,0 +1,102 @@
+// SPDX-License-Identifier: GPL-2.0
+
+/dts-v1/;
+
+#include "mt7621.dtsi"
+
+/ {
+	compatible = "mediatek,mt7621-soc";
+	model = "TELEOFIS LT70";
+
+	aliases {
+		serial0 = &uart0;
+		spi0 = &spi;
+	};
+
+	chosen {
+		stdout-path = &uart0;
+	};
+
+	leds {
+		compatible = "gpio-leds";
+		act {
+			label = "bottom:act";
+			gpios = <&gpio0 22 0>;
+			default-state = "on";
+		};
+		net {
+			label = "middle:net";
+			gpios = <&gpio0 23 0>;
+			default-state = "on";
+		};
+		power {
+			label = "top:power";
+			gpios = <&gpio0 24 0>;
+			default-state = "on";
+		};
+	};
+
+	keys {
+		compatible = "gpio-keys";
+		switch@0 {
+			label = "status";
+			gpios = <&gpio0 18 1>;
+			linux,code = <0x101>;
+		};
+	};
+
+	watchdog {
+		compatible = "linux,wdt-gpio";
+		gpios = <&gpio0 28 0>;
+		hw_algo = "toggle";
+		always-running;
+	};
+};
+
+&pinctrl {
+	state_default: pin_state {
+		gpios {
+			groups = "wdt", "rgmii2", "jtag";
+			function = "gpio";
+		};
+	};
+};
+
+&uart0 {
+	status = "okay";
+};
+
+&gpio1 {
+	status = "okay";
+};
+
+&spi {
+	status = "okay";
+	num-cs = <2>;
+
+	spi-flash@0 {
+		#address-cells = <1>;
+		#size-cells = <1>;
+		compatible = "jedec,spi-nor";
+		spi-max-frequency = <10000000>;
+		reg = <0>;
+	};
+};
+
+&eth {
+	status = "okay";
+};
+
+&mmc {
+	cap-sd-highspeed;
+
+	status = "okay";
+};
+
+&ssusb {
+	status = "okay";
+};
+
+&u3phy {
+	status = "okay";
+};
